{
  "expo": {
    "name": "Calvin's Latin Bible",
    "slug": "calvins-latin-bible",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/clb_app-icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.avolzer.calvins-latin-bible"
    },
    "android": {
      "permissions": [
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "INTERNET"
      ],
      "blockedPermissions": ["RECORD_AUDIO"],
      "versionCode": 2,
      "adaptiveIcon": {
        "foregroundImage": "./assets/clb_app-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.avolzer.calvinslatinbible"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": ["expo-asset"],
    "extra": {
      "eas": {
        "projectId": "316fa2dc-cdfe-4fc4-9207-ea06df50a920"
      }
    }
  }
}
